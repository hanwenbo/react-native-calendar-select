{
	"name": "react-native-calendar-select",
	"version": "0.1.2",
	"description": "Calendar select component for react-native, like Airbnb.",
	"main": "./Calendar.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Tinysymphony/react-native-calendar-select.git"
	},
	"scripts": {
		"start": "react-native start",
		"ios": "react-native run-ios",
		"android": "react-native run-android",
		"test": "jest",
		"coverage": "cat ./coverage/lcov.info | coveralls",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
		"glog": "cd ./node_modules/react-native/third-party/glog-0.3.4 && ./configure && make && make install & cd ../../../"
	},
	"keywords": [
		"react-native",
		"react-native-calendar-xg",
		"calendar"
	],
	"dependencies": {
		"moment": "^2.18.1"
	},
	"peerDependencies": {
		"prop-types": "^15.6.0",
		"react": "16.8.3",
		"react-native": "0.59.1"
	},
	"devDependencies": {
		"coveralls": "^2.13.1",
		"cz-conventional-changelog": "^2.0.0",
		"babel-preset-react-native": "4.0.0",
		"enzyme": "^3.2.0",
		"enzyme-adapter-react-16": "^1.1.0",
		"istanbul": "^0.4.5",
		"react": "^16.1.0",
		"react-addons-test-utils": "^15.6.2",
		"react-dom": "^16.2.0",
		"react-native": "^0.50.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "24.5.0",
		"jest": "24.5.0",
		"metro-react-native-babel-preset": "0.53.1",
		"react-test-renderer": "16.6.3"
	},
	"jest": {
		"preset": "react-native",
		"verbose": true,
		"collectCoverage": true,
		"moduleFileExtensions": [
			"js"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
